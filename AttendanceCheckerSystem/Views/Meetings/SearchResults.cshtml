@model IEnumerable<Student>
@{
    ViewData["Title"] = "SearchResults";
}

<h2>SearchResults</h2>

@if (Model.Count() == 0)
{
    <p>No students matching those terms were found!</p>
}
else
{
    <p>The following students were found:</p>
    <ul>
        @foreach (var s in Model)
        {
            <li>@s.FirstName : @s.LastName</li>

        }
    </ul>
}
